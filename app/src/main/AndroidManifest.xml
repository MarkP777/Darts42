<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gmp.android.darts42">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Darts42">
        <activity android:name=".ReviewChallenge"></activity>
        <activity android:name=".IssueChallenge" ></activity>
        <activity
            android:name=".InitialiseGame"
            android:label="@string/title_activity_initialise_game"
            android:theme="@style/Theme.Darts1" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>